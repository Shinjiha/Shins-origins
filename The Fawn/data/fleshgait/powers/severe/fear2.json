{
  "hidden": true,
  "type": "apoli:multiple",
  "kos": {
    "type": "origins:action_over_time",
    "interval": 20,
    "entity_action": {
      "type": "origins:area_of_effect",
      "radius": 16,
      "shape": "sphere",
      "bientity_action": {
        "type": "origins:damage",
        "amount": 0,
        "damage_type": "minecraft:starve"
      },
      "bientity_condition": {
        "type": "origins:and",
        "conditions": [
          {
            "type": "origins:actor_condition",
            "condition": {
              "type": "origins:resource",
              "resource": "origins:transform_shapeshift_resource",
              "comparison": "==",
              "compare_to": 0
            }
          },
          {
            "type": "origins:invert",
            "condition": {
              "type": "origins:can_see",
              "shape_type": "outline"
            }
          },
          {
            "type": "origins:invert",
            "condition": {
              "type": "origins:attack_target",
              "inverted": true
            }
          },
          {
            "type": "origins:target_condition",
            "condition": {
              "type": "origins:nbt",
              "nbt": "{AngerTime: 0}"
            }
          }
        ]
      }
    }
  },
  "animal_fear": {
    "type": "origins:action_over_time",
    "entity_action": {
      "type": "origins:and",
      "actions": [
        {
          "type": "origins:execute_command",
          "command": "tag @s add MobFear"
        },
        {
          "type": "apoli:area_of_effect",
          "bientity_action": {
            "type": "origins:and",
            "actions": [
              {
                "type": "origins:add_velocity",
                "z": 0.35,
                "set": true
              },
              {
                "type": "origins:target_action",
                "action": {
                  "type": "origins:execute_command",
                  "command": "execute rotated as @p[tag=MobFear] run tp @s ~ ~ ~ ~ ~"
                }
              }
            ]
          },
          "bientity_condition": {
            "type": "origins:and",
            "conditions": [
              {
                "type": "apoli:can_see"
              },
              {
                "type": "origins:target_condition",
                "condition": {
                  "type": "origins:and",
                  "conditions": [
                    {
                      "type": "origins:in_tag",
                      "tag": "fleshgait:hostile",
                      "inverted": true
                    },
                    {
                      "type": "origins:in_tag",
                      "tag": "fleshgait:neutral",
                      "inverted": true
                    },
                    {
                      "type": "origins:entity_type",
                      "entity_type": "player",
                      "inverted": true
                    },
                    {
                      "type": "origins:entity_type",
                      "entity_type": "armor_stand",
                      "inverted": true
                    },
                    {
                      "type": "origins:entity_type",
                      "entity_type": "villager",
                      "inverted": true
                    },
                    {
                      "type": "origins:in_tag",
                      "tag": "fleshgait:animal"
                    },
                    {
                      "type": "origins:on_block"
                    }
                  ]
                }
              }
            ]
          },
          "include_target": false,
          "radius": 8
        },
        {
          "type": "origins:execute_command",
          "command": "tag @s remove MobFear"
        }
      ]
    },
    "interval": 1
  }
}