{
    "hidden": true,
    "type": "origins:multiple",
    "fix": {
        "type": "origins:action_on_callback",
        "entity_action_chosen": {
            "type": "origins:apply_effect",
            "effects": [
                {
                    "effect": "minecraft:speed",
                    "duration": -1,
                    "is_ambient": false,
                    "show_particles": false,
                    "show_icon": false
                },
                {
                    "effect": "minecraft:jump_boost",
                    "duration": -1,
                    "is_ambient": false,
                    "show_particles": false,
                    "show_icon": false
                }
            ]
        }
    }, 
    "dmganimal": {
        "type": "origins:conditioned_attribute",
        "modifier": {
            "attribute": "minecraft:generic.attack_damage",
            "operation": "addition",
            "value": 6
        },
        "condition": {
            "type": "origins:and",
            "conditions": [
                {
                    "type": "origins:equipped_item",
                    "equipment_slot": "mainhand",
                    "item_condition": {
                        "type": "origins:ingredient",
                        "ingredient": {
                            "item": "minecraft:air"
                        }
                    }
                }
            ]
        }
    },
    "phase": {
        "type": "origins:action_over_time",
        "interval": 1,
        "entity_action": {
            "type": "origins:if_else_list",
            "actions": [
                {
                    "condition": {
                        "type": "origins:food_level",
                        "comparison": ">",
                        "compare_to": 16
                    },
                    "action": {
                        "type": "origins:execute_command",
                        "command": "origin set @p origins:hunger_stage fleshgait:full"
                    }
                },
                {
                    "condition": {
                        "type": "origins:food_level",
                        "comparison": ">",
                        "compare_to": 10
                    },
                    "action": {
                        "type": "origins:execute_command",
                        "command": "origin set @p origins:hunger_stage fleshgait:moderate"
                    }
                },
                {
                    "condition": {
                        "type": "origins:food_level",
                        "comparison": ">",
                        "compare_to": 4
                    },
                    "action": {
                        "type": "origins:execute_command",
                        "command": "origin set @p origins:hunger_stage fleshgait:severe"
                    }
                },
                {
                    "condition": {
                        "type": "origins:food_level",
                        "comparison": "<=",
                        "compare_to": 4
                    },
                    "action": {
                        "type": "origins:execute_command",
                        "command": "origin set @p origins:hunger_stage fleshgait:starving"
                    }
                }
            ]
        },
        "condition": {
            "type": "origins:origin",
            "origin": "fleshgait:fleshgait"
        }
    },
    "no_food": {
        "type": "origins:modify_food",
        "item_condition": {
            "type": "origins:meat",
            "inverted": true
        },
        "food_modifier": {
            "name": "No Nurishment",
            "operation": "set_total",
            "value": 0
        },
        "saturation_modifier": {
            "name": "No Nurishment",
            "operation": "set_total",
            "value": 0
        }
    },
    "rawmeat": {
        "type": "origins:modify_food",
        "prevent_effects": true,
        "item_condition": {
            "type": "origins:meat"
        },
        "food_modifier": {
            "name": "Minimal Nurishment",
            "operation": "set_total",
            "value": 1
        },
        "saturation_modifier": {
            "name": "No Nurishment",
            "operation": "set_total",
            "value": 0
        }
    },
    "live_feeding": {
        "name": "Live Feeding",
        "hidden": false,
        "description": "You can only eat what's still moving.",
        "type": "origins:self_action_on_kill",
        "damage_condition": {
            "type": "origins:and",
            "conditions": [
                {
                    "inverted": true,
                    "type": "origins:explosive"
                },
                {
                    "inverted": true,
                    "type": "origins:projectile"
                },
                {
                    "inverted": true,
                    "type": "origins:fire"
                }
            ]
        },
        "target_condition": {
            "type": "origins:in_tag",
            "tag": "fleshgait:animal",
            "inverted": true
        },
        "entity_action": {
            "type": "origins:heal",
            "amount": 1
        }
    },
    "cannibal": {
        "name": "Live Feeding",
        "hidden": false,
        "description": "You can only eat what's still moving.",
        "type": "origins:self_action_on_kill",
        "damage_condition": {
            "type": "origins:and",
            "conditions": [
                {
                    "inverted": true,
                    "type": "origins:explosive"
                },
                {
                    "inverted": true,
                    "type": "origins:projectile"
                },
                {
                    "inverted": true,
                    "type": "origins:fire"
                }
            ]
        },
        "target_condition": {
            "type": "origins:or",
            "conditions": [
                {
                    "type": "origins:entity_type",
                    "entity_type": "minecraft:villager"
                },
                {
                    "type": "origins:entity_type",
                    "entity_type": "minecraft:player"
                }
            ]
        },
        "entity_action": {
            "type": "origins:feed",
            "food": 20,
            "saturation": 20
        }
    },
    "yearning": {
        "type": "origins:self_action_on_kill",
        "damage_condition": {
            "type": "origins:and",
            "conditions": [
                {
                    "inverted": true,
                    "type": "origins:explosive"
                },
                {
                    "inverted": true,
                    "type": "origins:projectile"
                },
                {
                    "inverted": true,
                    "type": "origins:fire"
                }
            ]
        },
        "target_condition": {
            "type": "origins:in_tag",
            "tag": "fleshgait:animal"
        },
        "entity_action": {
            "type": "origins:feed",
            "food": -1,
            "saturation": 0
        }
    },
    "size": {
        "name": "Small",
        "hidden": false,
        "description": "You are slightly smaller than a human.",
        "type": "origins:action_on_callback",
        "entity_action_chosen": {
            "type": "origins:execute_command",
            "command": "/scale set 0.8 @s"
        },
        "execute_chosen_when_orb": true,
        "entity_action_lost": {
            "type": "origins:execute_command",
            "command": "/scale set 1 @s"
        },
        "entity_action_respawned": {
            "type": "origins:execute_command",
            "command": "/scale set 0.8 @s"
        }
    },
    "fall": {
        "type": "origins:modify_falling",
        "velocity": 0.08,
        "take_fall_damage": false
    }
}